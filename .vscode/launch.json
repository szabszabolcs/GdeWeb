    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: GdeWeb Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/GdeWeb/GdeWeb.csproj"
        },
        {
            "name": "C#: GdeWebAPI Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/GdeWebAPI/GdeWebAPI.csproj"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ],
    "compounds": [
        {
            "name": "Launch Full Stack",
            "configurations": [
                "C#: GdeWebAPI Debug",
                "C#: GdeWeb Debug"
            ],
            "stopAll": true,
            "preLaunchTask": "build"
        }
    ]
}